ectorAll(\".div_cadaProduto img \").forEach((item) => {\r\n    item.addEventListener(\"mouseleave\", () => {\r\n      item.style.backgroundColor = \"#e6e0e0\";\r\n    });\r\n  });\r\n}\r\n\r\nfunction ProdutosInfo(props) {\r\n  return (\r\n    <div className=\"col\">\r\n      <b>{props.produto.nome}</b>\r\n      <b>{props.produto.preco}</b>\r\n      <b>{props.produto.tipo}</b>\r\n      <b>\r\n        <img src={\"http://localhost:4000/files/\" + props.produto.imagem}></img>\r\n      </b>\r\n      <Link to=\"/Produtos\">\r\n        <button>ver</button>\r\n      </Link>\r\n    </div>\r\n  );\r\n}\r\n\r\nexport default withRouter(PaginaPrincipal);\r\n","C:\\Users\\thede\\Desktop\\tdw\\frontend\\src\\Componentes\\Header.js",["113"],"C:\\Users\\thede\\Desktop\\tdw\\frontend\\src\\api.js",[],"C:\\Users\\thede\\Desktop\\tdw\\frontend\\src\\Componentes\\Dashboard.js",["114"],"import React from \"react\";\r\nimport { Link,withRouter} from 'react-router-dom'\r\n\r\nclass Dashboard extends React.Component {\r\n    render() {\r\n        return (\r\n            <div>\r\n                <td> <Link to={`/Dashboard/add/`}><button>Adicionar</button></Link> </td>\r\n                <table className=\"table\">\r\n                    <thead>\r\n                        <tr>\r\n                            <td><b>ref</b></td>\r\n                            <td><b>Imagem</b></td>\r\n                            <td><b>Nome</b></td>\r\n                            <td><b>Tipo</b></td>\r\n                            <td><b>Preço</b></td>\r\n                            <td style={{ textAlign: \"center\" }} colSpan=\"2\"> <b>Ações</b></td>\r\n                        </tr>\r\n                    </thead>\r\n\r\n                    <tbody>\r\n                        {\r\n                            this.props.produtos.map((item, i) => {\r\n                                return (\r\n                                    <tr key={i}>\r\n                                        <td>{item.ref}</td>\r\n  