props.MouseLeave}\r\n                            src={\"http://localhost:4000/files/\" + pos.imagem}\r\n                          ></img>\r\n                          <div className=\"infoProduto\">\r\n                          <Link style={{ textDecoration: \"none\" }} to={\"/Produto/\" + pos.ref}><div className=\"nomeProduto\">{pos.nome}</div></Link>\r\n                            <div className=\"precoProduto\">\r\n                              {pos.preco + \"$\"}\r\n                            </div>\r\n                            <div>cor</div>\r\n                          </div>\r\n                        </div>\r\n                      </div>\r\n                    );\r\n                  })}\r\n                </div>\r\n              </section>\r\n           \r\n            </div>\r\n           \r\n              \r\n\r\n            \r\n        )\r\n    }\r\n}\r\n\r\nexport default Produtos;","C:\\Users\\pecarmo\\Desktop\\tdw\\frontend\\src\\Componentes\\FormVariantes.js",["143","144","145"],"import React from \"react\";\r\nimport { Link, withRouter } from \"react-router-dom\";\r\nimport \"../Switch.css\"\r\nimport api from \"../shared/api\";\r\nimport tamanhosCalcado from \"../shared/tamanhosCalcado\";\r\nimport tamanhosRoupa from \"../shared/tamanhosRoupa\";\r\nimport cor from \"../shared/cores\";\r\n\r\nlet file = '';\r\nvar imagemForm = new FormData();\r\n\r\nclass FormProduto extends React.Component {\r\n    constructor(props) {\r\n        super(props);\r\n        this.state = {\r\n            variante: {\r\n                id: null,\r\n                ref: this.props.match.params.id,\r\n                tamanho: \"XS\",\r\n                imagens: [],\r\n                cor: \"Branco\",\r\n                stock: \"\"\r\n            },\r\n            selectedCor: {},\r\n            imagens: [],\r\n            cores: [],\r\n            tamanhos: [],\r\n        }\r\n    }\r\n\r\n    componentDidMount() {\r\n        this.props.produtos.map((pos) => {\r\n    